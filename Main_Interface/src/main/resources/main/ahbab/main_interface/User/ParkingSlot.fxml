<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="547.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ahbab.main_interface.User.ParkingslotController">
   <children>
      <TableView fx:id="ParkingTable" editable="true" layoutX="73.0" layoutY="168.0" prefHeight="200.0" prefWidth="399.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ImageView fx:id="Car" blendMode="DARKEN" fitHeight="101.0" fitWidth="200.0" layoutX="222.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Vehicle.jpg" />
         </image>
      </ImageView>
      <Button fx:id="Reloadpark" alignment="CENTER" contentDisplay="CENTER" layoutX="247.0" layoutY="105.0" mnemonicParsing="false" onAction="#loadParkingData" text="Reload">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
