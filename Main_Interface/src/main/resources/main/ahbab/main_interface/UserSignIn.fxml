<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="176.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ahbab.main_interface.User.GuestScreenController">
   <children>
      <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleBack" text="← Back" />
      <TextField fx:id="nameofuser" alignment="CENTER" layoutX="25.0" layoutY="48.0" prefWidth="250.0" promptText="Full Name">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Text layoutX="111.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome Back" />
      <Button fx:id="signinuser" contentDisplay="CENTER" layoutX="123.0" layoutY="125.0" mnemonicParsing="false" onAction="#gotoGuestscreenaftersignin" text="Sign in" textAlignment="CENTER" />
      <PasswordField fx:id="passwordofuser" alignment="CENTER" layoutX="25.0" layoutY="79.0" prefHeight="23.0" prefWidth="250.0" promptText="Password">
         <font>
            <Font name="Advent Pro Medium" size="12.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
